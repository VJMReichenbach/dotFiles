{
	// Place your snippets for uwupp here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"dump state": {
		"prefix": "dumpState",
		"body": "dumpState()"
	},
	"function": {
	  "prefix": ["nyaa"],
	  "body": [
		  "nyaa *${1:functionName}(${2:vars})*",
		  "\t$0",
		  "wetuwn ${3:return}"
		],
	  "description": "A function"
	},
	"if": {
		"prefix": ["if","notices"],
		"body": [
			"$LINE_COMMENT This is an if statement",
			"*notices ${1:var1} ${3:condition} ${2:var2}*",
			"\t${0:$LINE_COMMENT content}",
			"stawp"
	]
	},
	"while": {
		"prefix": ["while", "OwO"],
		"body": [
			"$LINE_COMMENT This is an while loop",
			"OwO *notices ${1:var1} ${3:condition} ${2:var2}*",
			"\t${0:$LINE_COMMENT content}",
			"stawp"
	]
	},
	"print": {
		"prefix": ["nuzzels","print"],
		"body": "nuzzels($0)"
	},
	"equals": {
		"prefix": ["eqwall twoo", "equals"],
		"body": "eqwall twoo"
	},
	"greater than": {
		"prefix": ["gweatew twan", "greater"],
		"body": "gweatew twan"
	},
	"less than": {
		"prefix": ["wess twan", "less"],
		"body": "wess twan"
	},
	"is": {
		"prefix": ["iws","is"],
		"body": "iws",
		"description": "iws is used to define a variable. Usage: <var name> iws <value> Example: testVar iws 123"
	},
	"plus": {
		"prefix": ["plus", "pwus"],
		"body": "pwus"
	},
	"minus": {
		"prefix": ["minus", "minwus"],
		"body": "minwus"
	},
	"devide":{
		"prefix": ["devide", "diwide"],
		"body": "diwide"
	},
	"times": {
		"prefix": ["times", "twimes"],
		"body": "twimes"
	},
	"array": {
		"prefix": ["array", "awway"],
		"body": "awway<${1:length};${2:type}>"
	},
	"listen int": {
		"prefix": ["listenInt", "wistenInt"],
		"body": "wistenInt()"
	}
}